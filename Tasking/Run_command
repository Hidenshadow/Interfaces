# 0) 可选：创建虚拟环境（推荐）
python -m venv .venv
source .venv/bin/activate        # Windows: .venv\Scripts\activate
pip install --upgrade pip
pip install pandas numpy skyfield sgp4

# 1) 由 TLE 生成卫星能力骨架（sat_capabilities.json）
python make_sat_caps_from_tle.py \
  --tle satellites_10.tle \
  --out sat_capabilities.json

# 2) 生成候选观测（读取固定的 test.csv / sat_capabilities.json / satellites_10.tle）
python generate_candidates.py

# 3) Run
python plan_greedy.py
python plan_tabu.py
python plan_milp.py
python plan_mis.py

# 4) 导出“单行堆叠版”覆盖汇总
python analysis.py




#github update

git status                       # 看看改了什么（可选）
git add -A                       # 收集所有改动（新增/修改/删除）
git commit -m "feat: 你的更新说明"  # 本地提交
git pull --rebase origin main    # 先把远端的最新改动叠到前面（更干净）
git push                         # 推到 GitHub